{
  "name": "ctf",
  "version": "1.0.0",
  "description": "CTF interface",
  "main": "server.js",
  "repository": "https://github.com/jamjar919/ctf.git",
  "author": "James Paterson",
  "license": "None",
  "scripts": {
    "start": "npm-run-all delete-all build:server build:client start:server start:client logs",
    "logs": "pm2 logs",
    "delete-all": "pm2 delete all",
    "start:server": "pm2 start ./dist/server.js --name ctf-server",
    "start:client": "pm2 start \"next start ./src/client\" --name ctf-client",
    "build:client": "next build ./src/client",
    "build:client:dev": "next dev ./src/client",
    "build:server": "npx webpack --config webpack.server.config.js",
    "build:server:dev": "npx webpack -w --config webpack.server.config.js --mode=development",
    "test": "jest",
    "graphql": "graphql-codegen --config graphql-codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.7.13",
    "@apollo/server": "^4.7.0",
    "body-parser": "^1.20.2",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "formik": "^2.4.3",
    "graphql": "^16.6.0",
    "js-cookie": "^3.0.5",
    "mongodb": "^5.3.0",
    "next": "^13.3.4",
    "node-fetch": "^3.3.1",
    "pm2": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0",
    "victory": "^36.6.11",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-resolvers": "^3.2.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/google-map-react": "^2.1.7",
    "@types/js-cookie": "^3.0.3",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.3",
    "@types/victory": "^35.0.0",
    "babel-loader": "^9.1.2",
    "dotenv": "^16.0.3",
    "nodemon-webpack-plugin": "^4.8.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.3"
  },
  "engines": {
    "node": "18.16.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ]
  }
}
